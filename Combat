for i , v in pairs(getgc(true)) do 
    if type(v) == "table" and rawget(v,"FireRate") then
        v.FireRate = 3000
        v.Range = Vector2.new(99999999, 99999999)
        v.MagnetStrength = 600
        v.AmmoCapacity = math.huge
        v.ReloadTime = 0
        v.ReloadEmptyTime = 0
    end
end
